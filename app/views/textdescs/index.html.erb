<h1>Listing textdescs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Text</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @textdescs.each do |textdesc| %>
  <tr>
    <td><%= textdesc.name %></td>
    <td><%= textdesc.text %></td>
    <td><%= link_to 'Show', textdesc %></td>
    <td><%= link_to 'Edit', edit_textdesc_path(textdesc) %></td>
    <td><%= link_to 'Destroy', textdesc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Textdesc', new_textdesc_path %>
